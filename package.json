    "github-api": "3.0.0",
    "html-loader": "0.4.4",
    "http-server": "0.10.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-junit-reporter": "1.0.0",
    "karma-qunit": "1.2.1",
    "karma-sourcemap-loader": "0.3.7",

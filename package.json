    "survey-knockout": "^1.0.31"
  },
  "devDependencies": {
    "@types/ace": "0.0.32",
    "@types/bootstrap": "3.3.32",
    "@types/jquery": "2.0.41",
    "@types/knockout": "3.4.39",
    "@types/node": "7.0.4",
    "@types/qunit": "2.0.31",
    "ace-builds": "1.2.2",
    "ajv": "4.11.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-latest": "6.22.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "dotenv": "4.0.0",

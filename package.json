{
  "version": "1.1.28",
  "name": "survey-creator",
  "homepage": "https://surveyjs.io/Overview/Survey-Creator",
  "license": "https://surveyjs.io/Licenses#BuildSurvey",
  "repository": {
    "type": "git",
    "url": "https://github.com/surveyjs/survey-creator.git"
  },
  "scripts": {
    "start": "webpack-dev-server --env.buildType dev",
    "test": "karma start",
    "testsingle": "karma start --single-run",
    "test_ci": "karma start --single-run",
    "doc_gen": "node doc_generator/editor_docgenerator.js src/entries/index.ts",
    "doc_update": "chmod +x docupdate_npm.sh && ./docupdate_npm.sh",
    "testcafe": "concurrently \"http-server -p 7777\" \"testcafe chrome testCafe/\"",
    "testcafe_ci": "http-server -p 7777 --silent & testcafe chrome:headless --reporter minimal testCafe/",
    "testcafe_file": "concurrently \"http-server -p 7777\" \"testcafe chrome testCafe/tests/adorners.js\"",
    "release": "standard-version --message \"Release: %s\"",
    "build_dev": "webpack --env.buildType dev --env.libraryName SurveyCreator && webpack --env.buildType dev",
    "build_prod": "webpack --env.buildType dev --env.libraryName SurveyCreator && webpack --env.buildType prod --env.libraryName SurveyCreator && webpack --env.buildType dev && webpack --env.buildType prod",
    "watch_dev": "webpack --env.buildType dev --watch"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "knockout": "^3.5.0",
    "rmodal": "1.0.31",
    "sortablejs": "1.6.1",
    "survey-knockout": "^1.1.27"
  },
  "devDependencies": {
    "@types/ace": "0.0.32",
    "@types/bootstrap": "3.3.32",
    "@types/node": "7.0.4",
    "@types/qunit": "2.0.31",
    "ace-builds": "1.2.2",
    "ajv": "4.11.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-latest": "6.22.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "dotenv": "4.0.0",
    "dts-bundle": "0.7.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.0",
    "generate-json-webpack-plugin": "0.2.1",
    "github-api": "3.0.0",
    "html-loader": "0.4.4",
    "http-server": "0.10.0",
    "husky": "^1.2.1",
    "jquery": "^3.4.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-junit-reporter": "1.0.0",
    "karma-qunit": "1.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "node-sass": "^4.12.0",
    "puppeteer": "^1.19.0",
    "qunitjs": "2.2.0",
    "replace-in-file": "^3.4.3",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.1",
    "standard-version": "4.0.0",
    "style-loader": "0.13.1",
    "surveyjs-doc-generator": "git+https://github.com/surveyjs/surveyjs-doc-generator.git",
    "testcafe": "0.17.2",
    "ts-loader": "2.3.4",
    "ts-node": "3.3.0",
    "typescript": "3.6.2",
    "url-loader": "0.5.8",
    "val-loader": "0.5.0",
    "webpack": "2.2.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^2.9.1",
    "webpack-svgstore-plugin": "4.0.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run testsingle"
    }
  }
}

    "karma-webpack": "^5.0.0",
    "ng-packagr": "^13.0.0",
    "rxjs": "~7.4.0",
    "standard-version": "^8.0.1",
    "survey-angular-ui": "../../../survey-library/build/survey-angular-ui",
    "survey-core": "../../../survey-library/build/survey-core",
    "survey-creator-core": "../survey-creator-core/build",
    "tslib": "^2.3.0",

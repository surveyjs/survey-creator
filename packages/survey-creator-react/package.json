{
  "name": "survey-creator-react",
  "version": "1.8.63-beta.0",
  "description": "",
  "homepage": "https://surveyjs.io/Overview/Survey-Creator",
  "author": "Devsoft Baltic OU",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/surveyjs/survey-creator.git"
  },
  "scripts": {
    "start": "webpack-dev-server --env.buildType dev --open",
    "serve": "http-server --silent",
    "test": "jest",
    "test:update": "npm run test -- -u",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || echo",
    "test:dev": "jest --watch",
    "test_debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "doc:gen": "node doc_generator/editor_docgenerator.js src/entries/index.ts",
    "doc:update": "chmod +x docupdate_npm.sh && ./docupdate_npm.sh",
    "testcafe": "concurrently \"http-server --silent\" \"testcafe -c 4 chrome ../../testCafe/ --selector-timeout 1500\"",
    "testcafe:ci": "http-server --silent & testcafe chrome:headless ../../testCafe/ --selector-timeout 1500 ",
    "testcafe:file": "concurrently \"http-server --silent\" \"testcafe chrome ../../testCafe/designer/select-context.js --reporter minimal --debug-mode\"",
    "release": "standard-version --message \"Release: %s [skip ci]\" --prerelease beta",
    "remove-package-lock": "rimraf package-lock.json",
    "build": "webpack --env.buildType dev && webpack --env.buildType prod",
    "watch:dev": "webpack --env.buildType dev --watch",
    "watch:prod": "webpack --env.buildType prod --watch",
    "-prestorybook": "npm run test:generate-output",
    "storybook": "start-storybook -p 9000"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "6.0.21",
    "@storybook/addon-actions": "6.0.21",
    "@storybook/addon-backgrounds": "6.0.21",
    "@storybook/addon-docs": "6.0.21",
    "@storybook/addon-events": "6.0.21",
    "@storybook/addon-jest": "6.0.21",
    "@storybook/addon-knobs": "6.0.21",
    "@storybook/addon-links": "6.0.21",
    "@storybook/addon-storyshots": "6.0.21",
    "@storybook/addon-storysource": "6.0.21",
    "@storybook/addon-viewport": "6.0.21",
    "@survey/creator": "../survey-creator-core/build",
    "@types/ace": "0.0.44",
    "@types/jest": "23.3.3",
    "@types/jquery": "3.3.29",
    "@types/lodash": "4.14.121",
    "@types/node": "7.0.4",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.1",
    "ajv": "4.11.2",
    "chalk": "^3.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "^3.6.0",
    "dotenv": "4.0.0",
    "dts-bundle": "0.7.2",
    "github-api": "^3.3.0",
    "html-loader": "^0.4.4",
    "http-server": "^0.12.3",
    "jest": "^26.1.0",
    "jest-canvas-mock": "^2.2.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.12.0",
    "node-uuid": "1.4.7",
    "replace-in-file": "^6.1.0",
    "rimraf": "2.5.4",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.3",
    "standard-version": "^8.0.2",
    "storybook-addon-designs": "^5.4.2",
    "style-loader": "^1.2.1",
    "surveyjs-doc-generator": "git+https://github.com/surveyjs/surveyjs-doc-generator.git",
    "testcafe": "^1.14.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^8.0.15",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.2.2",
    "url-loader": "0.6.2",
    "val-loader": "0.5.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rmodal": "^1.0.36",
    "survey-core": "../../../survey-library/build/survey-core",
    "survey-react-ui": "../../../survey-library/build/survey-react-ui"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  }
}

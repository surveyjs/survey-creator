{
  "name": "survey-creator-knockout",
  "version": "1.9.20",
  "description": "",
  "homepage": "https://surveyjs.io/Overview/Survey-Creator",
  "author": "Devsoft Baltic OU",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/surveyjs/survey-creator.git"
  },
  "scripts": {
    "start": "webpack-dev-server --env.buildType dev --open",
    "serve": "http-server --silent",
    "test": "jest",
    "test:update": "npm run test -- -u",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || echo",
    "test:dev": "jest --watch",
    "test_debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "doc:gen": "node doc_generator/editor_docgenerator.js src/entries/index.ts",
    "doc:update": "chmod +x docupdate_npm.sh && ./docupdate_npm.sh",
    "testcafe": "concurrently \"http-server --silent\" \"testcafe -c 4 chrome ../../testCafe/ --selector-timeout 1500\"",
    "testcafe:file": "concurrently \"http-server --silent\" \"testcafe chrome ../../testCafe/designer/toolbox.ts --reporter minimal --selector-timeout 1500\"",
    "testcafe:a11y": "concurrently \"http-server --silent\" \"testcafe chrome ./testCafe/accessibility.js --selector-timeout 1500\"",
    "testcafe:ci": "http-server --silent & testcafe -c 4 -q chrome:headless ../../testCafe/ --selector-timeout 1500 --reporter minimal,dashboard",
    "visual-regression-tests": "testcafe chrome --disable-font-subpixel-positioning ../../visualRegressionTests/ --app \"http-server\" --screenshots ../../ --selector-timeout 1500 --reporter minimal,dashboard",
    "visual-regression-tests:ci": "testcafe chrome:headless -c 4 -q --disable-font-subpixel-positioning ../../visualRegressionTests/ --app \"http-server\" --screenshots ../../ --selector-timeout 1500 --reporter minimal,dashboard",
    "visual-regression-tests:file": "testcafe chrome:headless --disable-font-subpixel-positioning --app \"http-server\" ../../visualRegressionTests/tests/designer/pg-choices.ts --screenshots ../../ --reporter minimal --selector-timeout 1500",
    "release": "standard-version --message \"Release: %s [skip ci]\"",
    "remove-package-lock": "rimraf package-lock.json",
    "build": "webpack --env.buildType dev && webpack --env.buildType prod",
    "watch:dev": "webpack --env.buildType dev --watch",
    "watch:prod": "webpack --env.buildType prod --watch",
    "-prestorybook": "npm run test:generate-output",
    "storybook": "start-storybook -p 9000",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "6.0.21",
    "@storybook/addon-actions": "6.0.21",
    "@storybook/addon-backgrounds": "6.0.21",
    "@storybook/addon-docs": "6.0.21",
    "@storybook/addon-events": "6.0.21",
    "@storybook/addon-jest": "6.0.21",
    "@storybook/addon-knobs": "6.0.21",
    "@storybook/addon-links": "6.0.21",
    "@storybook/addon-storyshots": "file:./storybook-storyshot-knockout",
    "@storybook/addon-storysource": "6.0.21",
    "@storybook/addon-viewport": "6.0.21",
    "@storybook/knockout": "file:./storybook-knockout",
    "@types/ace": "0.0.44",
    "@types/jest": "23.3.3",
    "@types/jquery": "3.3.29",
    "@types/lodash": "4.14.121",
    "@types/node": "7.0.4",
    "ajv": "4.11.2",
    "axe-core": "^3.5.6",
    "axe-testcafe": "^3.0.0",
    "chalk": "^3.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "^3.6.0",
    "devextreme-screenshot-comparer": "^2.0.11",
    "dotenv": "4.0.0",
    "eslint": "^7.32.0",
    "github-api": "^3.3.0",
    "html-loader": "^0.4.4",
    "http-server": "^0.12.3",
    "jest": "^26.1.0",
    "jest-canvas-mock": "^2.2.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.12.0",
    "node-uuid": "1.4.7",
    "replace-in-file": "^6.1.0",
    "rimraf": "2.5.4",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.3",
    "standard-version": "^8.0.2",
    "storybook-addon-designs": "^5.4.2",
    "style-loader": "^1.2.1",
    "surveyjs-doc-generator": "git+https://github.com/surveyjs/surveyjs-doc-generator.git",
    "testcafe": "^1.16.0",
    "testcafe-reporter-dashboard": "^0.2.4-rc.1",
    "ts-jest": "^26.1.1",
    "ts-loader": "^8.0.15",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.2.2",
    "url-loader": "0.6.2",
    "val-loader": "0.5.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-svgstore-plugin": "^4.1.0",
    "colors": "1.4.0"
  },
  "dependencies": {
    "knockout": "^3.5.1",
    "rmodal": "^1.0.36",
    "survey-core": "../../../survey-library/build/survey-core",
    "survey-creator-core": "../survey-creator-core/build",
    "survey-knockout-ui": "../../../survey-library/build/survey-knockout-ui"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  }
}
